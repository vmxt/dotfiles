!! URxvt Appearance
URxvt.font: xft:screen:style:Regular:size=11
URxvt.boldFont: xft:screen:style:Bold:size=11
URxvt.italicFont: xft:screen:style:Italic:size=11
URxvt.boldItalicfont: xft:screen:style:Bold Italic:size=11
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 76x22
URxvt.internalBorder: 14
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
!!URxvt.depth: 24
URxvt.iso14755: false

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

!! Copy and Paste with Ctrl-Shift-c and Ctrl-Shift-v
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard
!!
URxvt.urlLauncher: icecat
URxvt.underlineURLs: true
URxvt.urlButton: 1
